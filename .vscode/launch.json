  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch(ZanProxy)",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "sourceMaps": true,
      "env": {
        "DEBUG": "1",
      },
      "runtimeVersion": "10.16.3",
      "args": [
        "-r",
        "ts-node/register",
        "--nolazy",
        "${workspaceRoot}/src/bin/index.ts",
        "--no-update",
        "--no-sync",
      ]
    }
  ]
}
